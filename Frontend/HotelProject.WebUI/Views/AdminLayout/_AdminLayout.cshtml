
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("HeadPartial.cshtml")

<body>

    @await Html.PartialAsync("PreloaderPartial.cshtml")

    
    <div id="main-wrapper">

       
        @await Html.PartialAsync("NavHeaderPartial.cshtml")
        
        @await Html.PartialAsync("HeaderPartial.cshtml")
        
        @await Html.PartialAsync("SidebarPartial.cshtml")
        
        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">

            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                @RenderBody()  
                            </div>
                        </div>
                    </div>
                </div>
            </div>
           
        </div>
        
        @await Html.PartialAsync("FooterPartial.cshtml")
        
    </div>
    
    @await Html.PartialAsync("ScriptPartial.cshtml")

</body>

</html>

